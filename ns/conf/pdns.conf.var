loglevel=5
log-dns-queries=${DESECSLAVE_LOGDNSQUERIES}
setgid=pdns
setuid=pdns
slave=no
soa-minimum-ttl=60
version-string=powerdns
carbon-server=${DESECSLAVE_CARBONSERVER}
carbon-ourname=${DESECSLAVE_CARBONOURNAME}

launch=gmysql
gmysql-host=db
gmysql-port=
gmysql-dbname=pdns
gmysql-user=pdns
gmysql-password=${DESECSLAVE_DB_PASSWORD_pdns}
gmysql-dnssec=yes
