loglevel=5
api=yes
api-key=${DESECSLAVE_NS_APIKEY}
setgid=pdns
setuid=pdns
slave=yes
soa-minimum-ttl=60
version-string=powerdns
webserver=yes
webserver-address=10.16.3.3
webserver-allow-from=10.16.3.4
carbon-server=${DESECSLAVE_CARBONSERVER}
carbon-ourname=${DESECSLAVE_CARBONOURNAME}

launch=gsqlite3
gsqlite3-database=/var/lib/powerdns/pdns.sqlite3
gsqlite3-dnssec=on
